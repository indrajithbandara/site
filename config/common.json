{
    "hosts": {
        "back": "0.0.0.0",
        "front": "0.0.0.0",
        "socket": "0.0.0.0"
    },
    "ports": {
        "back": 8080,
        "front": 8000,
        "socket": 8888
    },
    "endpoints": {
        "graphql": {
            "protocol": "http",
            "hostname": "${hosts.back}",
            "port": "${ports.back}",
            "pathname": "/graphql",
            "slashes": true
        },
        "socket": {
            "protocol": "ws",
            "hostname": "${hosts.socket}",
            "port": "${ports.socket}",
            "pathname": "/socket",
            "slashes": true
        }
    }
}
