{
    "services": [
        "users",
        "tacos"
    ],
    "auth": {
        "path": "/auth",
        "service": "users",
        "local": {
            "entity": "user",
            "usernameField": "email",
            "passwordField": "password"
        },
        "jwt": {
            "header": {
                "typ": "access"
            },
            "subject": "anonymous",
            "issuer": "feathers",
            "algorithm": "HS256"
        }
    }
}
