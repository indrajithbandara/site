{
    "server": {
        "host": "${hosts.public}",
        "port": "${ports.back}"
    },
    "services": [
        "users",
        "tacos"
    ],
    "graphql": {
        "host": "${hosts.public}",
        "port": "${ports.socket}",
        "path": "${endpoints.graphql}"
    },
    "auth": {
        "path": "${endpoints.auth}",
        "service": "users",
        "local": {
            "entity": "user",
            "usernameField": "email",
            "passwordField": "password"
        },
        "jwt": {
            "header": {
                "typ": "access"
            },
            "subject": "anonymous",
            "issuer": "feathers",
            "algorithm": "HS256"
        }
    }
}
